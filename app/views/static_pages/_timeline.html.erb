<div class="timeline">
  <% items.each do |item| %>
    <div class="timeline-item">
      <div class="timeline-dot"></div>
      <div class="timeline-content">
        <div class="timeline-date"><%= item[:timestamp] %></div>
        <div class="timeline-card">
          <h5><%= item[:title] %></h5>
          <p><%= item[:content] %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
